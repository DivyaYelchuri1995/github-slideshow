class Name:                 #Defining a class and creating an object in Python
    FirstName="Divya"
    #MiddleName=None
    LastName="Yelchuri"
    def Info(self):
        print("Dob is",DOB)
        global Age              #Using global keyword  variable to access it outside method
        Age=2020-DOB
DOB=1995                    #global variable
ob1=Name()                  #object1 creation
ob2=Name()                  #object 2 creation
print(ob1.FirstName,ob1.LastName,end="\n")          #object-fleid invocation
#ob1.Info()                          #object-method invocation
Name.Info(ob1)                         #method invocation without using object or self in method  definition
print("Age is",Age)
#print(ob1.Age)--------> Not right statement because Age is a local variable of the function Info() when the fn invoked automatically the variable is invoked
